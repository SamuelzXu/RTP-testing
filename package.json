{
  "name": "replicant-rtp-challenge",
  "version": "1.0.0",
  "license": "UNLICENCED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.inf.replicant.ai/replicant/rtp-challenge.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "receive": "ts-node ./src/receiver.ts",
    "transmit": "ts-node ./src/transmitter.ts",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint src --ext .ts",
    "fix": "npm run fix:prettier && npm run fix:lint",
    "fix:prettier": "prettier --write .",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "jest --rootDir src"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.6.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}
